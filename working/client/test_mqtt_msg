write coil

mosquitto_pub -p 1883 -h localhost -t "test" -m '{"role":"client","protocol":"tcp","ip":"127.0.0.1","port":1502,"transaction_delay":1000,"timeout":1000,"name":"test1","type":0,"slaveid":1,"address":304,"boolValue":1,"invalid_function":0}'


read coils

mosquitto_pub -p 1883 -h localhost -t "test" -m '{"role":"client","protocol":"tcp","ip":"127.0.0.1","port":1502,"transaction_delay":1000,"timeout":1000,"name":"test1","slaveid":3,"area":0,"address":304,"quantity":1,"scantime":500,"byteswap":0,"wordswap":0}'
